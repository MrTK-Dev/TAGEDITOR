<Window x:Class="ID3_Tag_Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ID3_Tag_Editor"
        mc:Ignorable="d"
        Title="TAGEDITOR by MrTK" Height="450" Width="800">
    <Grid>
        <Button Content="Rename" HorizontalAlignment="Left" Margin="335,200,0,0" VerticalAlignment="Top" Width="75" Click="OutputCount"/>
        <Button Content="Image" HorizontalAlignment="Left" Margin="335,270,0,0" VerticalAlignment="Top" Width="75" Click="ChangeImage"/>
        <StackPanel x:Name="panel_SideMenu" HorizontalAlignment="Left" Height="309" Margin="0,100,0,0" VerticalAlignment="Top" Width="100" Background="#FF2A2A2A">
            <Button Content="Button" HorizontalAlignment="Left" Height="40" Margin="0,0,0,0" Width="100"/>
        </StackPanel>
        <Image HorizontalAlignment="Left" Height="100" Margin="0,0,0,0" VerticalAlignment="Top" Width="100" Source="MrTK_Logo.png"/>

    </Grid>
</Window>
